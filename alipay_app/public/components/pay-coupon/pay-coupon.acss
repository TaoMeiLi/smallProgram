  background: #fff;
  width: 100%;
  overflow: hidden;
  bottom: 0;
  border-top-left-radius: 12rpx;
  border-top-right-radius: 12rpx;
  z-index: 333;
  min-height: 300rpx;
  height: 944rpx;
}
.pay-coupon-component .coupon-modal .modal-title {
  padding: 30rpx 0;
  text-align: center;
  position: relative;
}
.pay-coupon-component .coupon-modal .modal-title text {
