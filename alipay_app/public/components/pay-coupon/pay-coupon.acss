.pay-coupon-component {
  padding: 0rpx 30rpx;
  background: #fff;
}
.pay-coupon-component .weui-cell {
  min-height: 110rpx;
  box-sizing: border-box;
  padding: 0px;
}
.pay-coupon-component .weui-cell .weui-cell__bd .title {
  font-size: 28rpx;
  color: #000;
  line-height: 36rpx;
  display: flex;
  flex-direction: row;
  justify-content: start;
}
.pay-coupon-component .weui-cell .weui-cell__bd .title .coupon-num {
  display: inline-block;
  font-size: 20rpx;
  color: #FFFAEB;
  width: auto;
  height: 36rpx;
  line-height: 36rpx;
  background-image: linear-gradient(-229deg, #FF5B28 0%, #FA4D3B 100%);
  border-radius: 4rpx;
  position: relative;
  font-weight: 200;
  padding-right: 8rpx;
  padding-top: -4rpx;
  padding-left: 32rpx;
  margin-left: 10rpx;
  vertical-align: middle;
}
.pay-coupon-component .weui-cell .weui-cell__bd .title .coupon-num:after {
  position: absolute;
  content: "";
  width: 18rpx;
  height: 20rpx;
  background: url('assets/icon_lable_coupon.svg') no-repeat center;
  background-color: #ff5b28;
  background-size: 100%;
  left: 8rpx;
  top: 8rpx;
}
.pay-coupon-component .coupon-onOff switch {
  transform: scale(0.65) translate(30rpx);
}
.pay-coupon-component .show-coupon {
  padding: 0 30rpx 34rpx 0;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.pay-coupon-component .show-coupon .coupon-cell-l {
  font-size: 24rpx;
  color: #9F9F9F;
  font-weight: 200;
}
.pay-coupon-component .show-coupon .coupon-cell-l text {
  font-size: 20rpx;
  line-height: 20rpx;
  display: inline-block;
  border: 1rpx solid #A7A7A7;
  border-radius: 6rpx;
  padding: 8rpx;
}
.pay-coupon-component .show-coupon .coupon-cell-ft {
  font-size: 26rpx;
  color: #666;
  text-align: right;
  line-height: 24rpx;
  position: relative;
}
.pay-coupon-component .show-coupon .coupon-cell-ft:before {
  position: absolute;
  content: "";
  width: 20rpx;
  height: 20rpx;
  border-top: 2rpx solid #A7A7A7;
  border-right: 2rpx solid #A7A7A7;
  transform: rotate(-315deg);
  right: -20rpx;
}
.pay-coupon-component .show-coupon .discount {
  color: #FA5337;
  font-family: CoreSansD45Medium;
  font-size: 30rpx;
}
.pay-coupon-component .coupon-modal {
  left: 0;
